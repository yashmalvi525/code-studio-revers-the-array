

#include <iostream>
#include<math.h>  //from a specific position
using namespace std;
void reverse(int arr[],int n){
    int s=m+1;
  int  e=n-1;
    while(s<=e){
        swap(arr[s],arr[e]);
        s++;
        e--;
    }
}
void print(int arr[],int n){
    for(int i=0;i<5;i++){
cout<<arr[i]<<" ";
}cout<<endl;

}

int main()
{
   int arr[5]={1,2,3,4,5};
reverse(arr,5);
print(arr,5);
